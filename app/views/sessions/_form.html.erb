<%= form_for(@session) do |f| %>
  <%= f.error_messages :class => "error", :id => nil%>

  <p>
    <%= f.label :session_date %><br />
    <%= f.date_select :session_date %>
  </p>
  <p>
    <%= f.label :circa %><br />
    <%= f.check_box :circa %>
  </p>
  <p>
    <%= f.label :text_date %><br />
    <%= f.text_field :text_date %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :country_id %><br />
    <%= f.collection_select :country_id, Country.all, :id, :country, {:selected => 233 }  %>
  </p>
  <p>
    <%= f.label :state_id %><br />
    <%= f.collection_select :state_id, State.all, :id, :state, {:selected => 37 } %>
  </p>
  <p>
    <%= f.label :session_type_id %><br />
    <%= f.collection_select :session_type_id, SessionType.all, :id, :session_type %>
  </p>
  <p>
    <%= f.label :venue %><br />
    <%= f.text_field :venue %>
  </p>
  <p>
    <%= f.submit 'Save' %>
  </p>
<% end %>

